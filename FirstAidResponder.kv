#: import FadeTransition kivy.uix.screenmanager.FadeTransition

ScreenManagement:
    transition: FadeTransition(duration=0.2)
    LoadingPage:
    StartPage:
    LoginPage:
    RegisterPage:
    HomeMenu:
    FirstAidMenu1:
    FirstAidMenu2:
    CameraMenu:
    ProcessingPage:
    BruisePage:
    AbrasionPage:
    BurnPage:
    MinorWoundPage:
    BleedingPage:
    SprainPage:
    AmbulanceMenu1:
    AmbulanceMenu2:
    SettingsMenu1:
    SettingsMenu2:

<LoadingPage>:
    name: 'loadingpage'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'MobileAppBG.png'
    
    Image:
        id: loading_image
        size_hint: 0.2, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}

<StartPage>:
    name: 'startpage'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'START.png' 
    
    FloatLayout:
        Button:
            text: 'Log-in'
            size_hint: 0.4, 0.06
            pos_hint: {'center_x': 0.5,'y': 0.34}
            font_size: 25
            font_name: 'josefin-sans.bold.ttf'
            color: (1, 1, 1)
            background_color:(1, 0, 0, 1)
            halign: 'center'
            valign: 'center'
            on_release: app.root.current = 'loginpage'
        
        Button:
            text: 'Register'
            size_hint: 0.4, 0.06
            pos_hint: {'center_x': 0.5,'y': 0.26}
            font_size: 25
            font_name: 'josefin-sans.bold.ttf'
            color: (1, 1, 1)
            background_color:(1, 0, 0, 1)
            halign: 'center'
            valign: 'center'
            on_release: app.root.current = 'registerpage'

<LoginPage>:
    name: 'loginpage'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'LOG-IN.png' 
    
    FloatLayout:
        TextInput:
            id: email_input
            size_hint: 0.8, 0.06
            pos_hint: {'center_x': 0.5, 'center_y': 0.54}
            multiline: False
            background_color: (1, 1, 1, 1)  # White background for input
            foreground_color: (0, 0, 0, 1)  # Black text
            font_name: 'josefin-sans.bold.ttf'
            font_size: 20

        TextInput:
            id: password_input
            size_hint: 0.8, 0.06
            pos_hint: {'center_x': 0.5, 'center_y': 0.43}
            multiline: False
            #password: True  # Mask the input
            background_color: (1, 1, 1, 1)  # White background for input
            foreground_color: (0, 0, 0, 1)  # Black text
            font_name: 'josefin-sans.bold.ttf'
            font_size: 20

        Button:
            text: 'Log-in'
            size_hint: 0.4, 0.06
            pos_hint: {'center_x': 0.5,'y': 0.31}
            font_size: 25
            font_name: 'josefin-sans.bold.ttf'
            color: (1, 1, 1)
            background_color:(1, 0, 0, 1)
            halign: 'center'
            valign: 'center'
            on_release: root.login_user()

        Button:
            text: '<'
            size_hint: 0.120, 0.05
            pos_hint: {'center_x': 0.12,'y': 0.92}
            font_size: 20
            font_name: 'josefin-sans.bold.ttf'
            color: (1, 1, 1)
            background_color:(1, 0, 0, 1)
            halign: 'center'
            valign: 'center'
            on_release: app.root.current = 'startpage'

