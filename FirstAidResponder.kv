#: import FadeTransition kivy.uix.screenmanager.FadeTransition

ScreenManagement:
    transition: FadeTransition(duration=0.2)
    LoadingPage:
    HomeMenu:
    FirstAidMenu1:
    FirstAidMenu2:
    CameraMenu:
    ProcessingPage:
    BruisePage:
    AbrasionPage:
    BurnPage:
    MinorWoundPage:
    BleedingPage:
    SprainPage:
    AmbulanceMenu1:
    AmbulanceMenu2:

<LoadingPage>:
    name: 'loadingpage'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'MobileAppBG.png'
    
    Image:
        id: loading_image
        size_hint: 0.2, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}

<HomeMenu>:
    name: 'homemenu'
    canvas.before:
        Rectangle:
            size:self.size
            source: 'BG MAIN.png' 
        
    FloatLayout:
        Button:
            size_hint: 0.195, 0.07
            pos_hint: {'x': 0.05,'y': 0.0045}
            background_normal: 'HomeMenu2.png' 
            background_down: 'HomeMenu2.png'    
            on_release: app.root.current = 'homemenu'
    
        Button:
            size_hint: 0.195, 0.07
            pos_hint: {'x': 0.27,'y': 0.0045}
            background_normal: 'CameraMenu1.png' 
            background_down: 'CameraMenu1.png'    
            on_release: app.root.current = 'cameramenu'
    
        Button:
            size_hint: 0.195, 0.07
            pos_hint: {'x': 0.5,'y': 0.0045}
            background_normal: 'FirstAidMenu1.png'
            background_down: 'FirstAidMenu1.png'    
            on_release: app.root.current = 'firstaidmenu1'
    
        Button:
            size_hint: 0.195, 0.07
            pos_hint: {'x': 0.75,'y': 0.0045}
            background_normal: 'AmbulanceMenu1.png' 
            background_down: 'AmbulanceMenu1.png'    
            on_release: app.root.current = 'ambulancemenu1'
        
        Image:
            size_hint: 0.90, 0.2
            pos_hint: {'center_x': 0.5,'center_y': 0.82}
            source: 'HomeText.png'      
        
        Image:
            id: clock_image
            size_hint: 0.90, 0.16
            pos_hint: {'center_x': 0.5,'center_y': 0.69} 
            source: 'ClockBox.png' 

        Label:
            id: clock_label
            size_hint: 0.90, 0.16
            pos_hint: {'center_x': 0.5,'center_y': 0.69}
            font_size: 45
            font_name: 'josefin-sans.bold.ttf'
            color: (1, 1, 1)

        Image:
            size_hint: 0.90, 0.35
            pos_hint: {'center_x': 0.5,'center_y': 0.46} #Change the Calendar
            source: 'October.png' 
        
        Image:
            size_hint: 0.90, 0.2
            pos_hint: {'center_x': 0.5,'center_y': 0.2}
            source: 'Hotlines.png'         

